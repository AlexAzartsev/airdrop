{
    "name": "hammerstone/airdrop",
    "description": "A Laravel package to deploy your application faster by skipping asset compilation when possible.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Francis",
            "email": "aaron@hammerstonehq.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "7.*|8.*",
        "illuminate/filesystem": "7.*|8.*",
        "illuminate/console": "7.*|8.*"
    },
    "require-dev": {
        "orchestra/testbench": "5.*|6.*",
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "Hammerstone\\Airdrop\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hammerstone\\Airdrop\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AirdropServiceProvider"
            ]
        }
    }
}
